extends layout

block content
  h1= title
  - var results = [{driver:"Max", constructor: "Red Bull", ranking:1}, {driver:"Alonso", constructor: "Aston Martin", ranking:3},{driver:"Hamilton", constructor: "MERCEDES", ranking:4}]
  div(class="container-fluid") 
  div(class="row")
  -
  each item,index in results
    div(class="col-sm-2 ") #{item.driver} - #{item.constructor} - #{item.ranking}
  
